{
  "name": "zephyra",
  "version": "1.0.0",
  "description": "Autonomous Stair-Climbing Rover Control System",
  "private": true,
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "dev:server": "npm run dev --workspace=server",
    "dev:client": "npm run dev --workspace=client",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
